<%- include('./includes/header') %>


        <!-- Content -->
        <div class="content">
            <!-- Animated -->
            <div class="animated fadeIn">
                       
                <div class="col-md-4">
                    <section class="card">
                        <div class="twt-feed" style="background-image: url('https://i.ibb.co/0YtNrpf/190-1906482-zinc-ore-gold-ore-minecraft-png.jpg');">
                            <div class="corner-ribon black-ribon">
                                <span class="badge badge-danger">16$</span>
                            </div>

                            <div class="media">
                                <a href="#">
                                    <img class="align-self-center rounded-circle mr-3" style="width:85px; height:85px;" alt="" src="https://i.ibb.co/0YtNrpf/190-1906482-zinc-ore-gold-ore-minecraft-png.jpg">
                                </a>
                                <div class="media-body">
                                    <h2 class="text-white display-6">Normal</h2>
                                    <p class="text-light">16$</p>
                                </div>
                            </div>        
                        </div>
                        <div class="weather-category twt-category">
                            <ul>
                                <li class="active">
                                    <h5>8GB</h5>
                                    Ram
                                </li>
                                <li>
                                    <h5>30GB</h5>
                                    Disk
                                </li>
                                <li>
                                    <h5>400%</h5>
                                    Cpu
                                </li>
                            </ul>
                        </div>
                        <div class="twt-write col-sm-12">
                            <button type="button" class="btn btn-outline-success btn-sm"><i class="fa-solid fa-store"></i>&nbsp; Buy For 16$</button>
                        </div>
                        <footer class="twt-footer">
                            <b>Free</b>
                            <span class="pull-right">
                                Database, backup, Port
                            </span>
                        </footer>
                    </section>
                </div>

            <!-- .animated -->
        </div>
        <!-- /.content -->
        <div class="clearfix"></div>
        <!-- Footer -->
        <footer class="site-footer">
            <div class="footer-inner bg-white">
                <div class="row">
                    <div class="col-sm-6">
                        Copyright &copy; 2024 <%= AppName %>
                    </div>
                    <div class="col-sm-6 text-right">
                        Designed by <a href="https://github.io/AlphaCtyl">Abhiram</a>
                    </div>
                </div>
            </div>
        </footer>
        <!-- /.site-footer -->
    </div>
    <!-- /#right-panel -->

    <!-- Scripts -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-match-height@0.7.2/dist/jquery.matchHeight.min.js"></script>
    <script src="assets/js/main.js"></script>
    <!--  Chart js -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.7.3/dist/Chart.bundle.min.js"></script>
    <!--Flot Chart-->
    <script src="https://cdn.jsdelivr.net/npm/jquery.flot@0.8.3/jquery.flot.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/flot-spline@0.0.1/js/jquery.flot.spline.min.js"></script>
    <!-- local -->
    <script src="assets/js/widgets.js"></script>
</body>
</html>
