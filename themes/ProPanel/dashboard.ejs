<%- include('./includes/header') %>

        <!-- Content -->
        <div class="content">
            <!-- Animated -->
            <div class="animated fadeIn">
                          <!-- Status -->
                          <div class="row">
                            <%- include('./includes/status') %>
                        </div>
                 <div class="row">
                            <div class="col-lg-3 col-md-6">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="stat-widget-five">
                                            <div class="stat-icon dib flat-color-4">
                                                <i class="fa-solid fa-route"></i>
                                            </div>
                                            <div class="stat-content">
                                                <div class="text-left dib">
                                                    <div class="stat-text"><%= userServersCount.totalPorts %>/<%= packageport + userresources.row.ports %></div>
                                                    <div class="stat-heading">Ports</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="col-lg-3 col-md-6">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="stat-widget-five">
                                            <div class="stat-icon dib flat-color-4">
                                                <i class="fa-solid fa-database"></i>
                                            </div>
                                            <div class="stat-content">
                                                <div class="text-left dib">
                                                    <div class="stat-text"><%= userServersCount.totalDatabase %>/<%= packagedatabase + userresources.row.database %></div>
                                                    <div class="stat-heading">Database</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="col-lg-3 col-md-6">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="stat-widget-five">
                                            <div class="stat-icon dib flat-color-4">
                                                <i class="fa-solid fa-box-open"></i>
                                            </div>
                                            <div class="stat-content">
                                                <div class="text-left dib">
                                                    <div class="stat-text"><%= userServersCount.totalBackup %>/<%= packagebackup + userresources.row.backup %></div>
                                                    <div class="stat-heading">Backup</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-6">
                                <div class="card">
                                    <div class="card-header">
                                        <strong class="card-title">Basic Table</strong>
                                    </div>
                                    <div class="card-body">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                  <th scope="col">Name</th>
                                                  <th scope="col">Cpu</th>
                                                  <th scope="col">Ram</th>
                                                  <th scope="col">Disk</th>
                                                  <th scope="col">manage</th>
                                              </tr>
                                          </thead>
                                          <tbody>
                                  <% userServers.forEach(server => { %>
                                            <tr>
                                                <th scope="row"><%= server.name %></th>
                                                <td><%= server.cpu %></td>
                                                <td><%= server.ram %></td>
                                                <td><%= server.disk %></td>

<td><button onclick="window.location.href='./manage?id=<%= server.identifier %>'" type="button" class="btn btn-success btn-sm"><i class="fa-solid fa-gears"></i>&nbsp; manage</button>
    <% if (settings.store.renewals.status == true) { %>
    <button onclick="window.location.href='./renew?id=<%= server.id %>'" type="button" class="btn btn-success btn-sm"><i class="fa-solid fa-gears"></i>&nbsp; Renew</button>
          <% } %>

                                                </td>
                                            </tr>
                                            <% }); %>
                                        </tbody>
                                        
                                    </table>
                                </div>
                            </div>
                        </div>
                        
                            </div>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="card">
                                        <div class="card-header">
                                            <h4>Basic</h4>
                                        </div>
                                        <div class="card-body">
                                            <div class="custom-tab">
        
                                                <nav>
                                                    <div class="nav nav-tabs" id="nav-tab" role="tablist">
                                                        <a class="nav-item nav-link active" id="custom-nav-home-tab" data-toggle="tab" href="#custom-nav-home" role="tab" aria-controls="custom-nav-home" aria-selected="true">Home</a>
                                                        <a class="nav-item nav-link" id="custom-nav-profile-tab" data-toggle="tab" href="#custom-nav-profile" role="tab" aria-controls="custom-nav-profile" aria-selected="false">Profile</a>
                                                        <a class="nav-item nav-link" id="custom-nav-contact-tab" data-toggle="tab" href="#custom-nav-contact" role="tab" aria-controls="custom-nav-contact" aria-selected="false">Contact</a>
                                                    </div>
                                                </nav>
                                                <div class="tab-content pl-3 pt-2" id="nav-tabContent">
                                                    <div class="tab-pane fade show active" id="custom-nav-home" role="tabpanel" aria-labelledby="custom-nav-home-tab">
                                                        <p>Help</p>
                                                    </div>
                                                    <div class="tab-pane fade" id="custom-nav-profile" role="tabpanel" aria-labelledby="custom-nav-profile-tab">
                                                        <p>help2</p>
                                                    </div>
                                                    <div class="tab-pane fade" id="custom-nav-contact" role="tabpanel" aria-labelledby="custom-nav-contact-tab">
                                                        <p>help3.</p>
                                                    </div>
                                                </div>
        
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Status -->
                </div><!-- .row -->
            </div>
            <!-- .animated -->
        </div>
        <!-- /.content -->
        <div class="clearfix"></div>
        <!-- Footer -->
        <footer class="site-footer">
            <div class="footer-inner bg-white">
                <div class="row">
                    <div class="col-sm-6">
                        Copyright &copy; 2024 <%= AppName %>
                    </div>
                    <div class="col-sm-6 text-right">
                        Designed by <a href="https://github.io/AlphaCtyl">Abhiram</a>
                    </div>
                </div>
            </div>
        </footer>
        <!-- /.site-footer -->
    </div>
    <!-- /#right-panel -->

    <!-- Scripts -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-match-height@0.7.2/dist/jquery.matchHeight.min.js"></script>
    <script src="assets/js/main.js"></script>
    <!--  Chart js -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.7.3/dist/Chart.bundle.min.js"></script>
    <!--Flot Chart-->
    <script src="https://cdn.jsdelivr.net/npm/jquery.flot@0.8.3/jquery.flot.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/flot-spline@0.0.1/js/jquery.flot.spline.min.js"></script>
    <!-- local -->
    <script src="assets/js/widgets.js"></script>
</body>
</html>
